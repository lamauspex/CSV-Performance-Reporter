# Конфигурационные переменные для CSV Performance Reporter

# Настройки отчетов
DEFAULT_REPORT_TYPE=performance

# Пути к файлам данных
DEMO_DATA_FILE=data/employees1.csv
TEST_DATA_FILE=data/employees2.csv

# Настройки вывода
TABLE_FORMAT=grid
MAX_OUTPUT_WIDTH=120

# Настройки валидации
MIN_PERFORMANCE=0
MAX_PERFORMANCE=5
MIN_EXPERIENCE_YEARS=0

# Настройки сортировки
SORT_BY_PERFORMANCE=true
SORT_ORDER=desc

# Настройки тестирования
TEST_COVERAGE_THRESHOLD=80